{
  "scripts": {
    "dev": "NODE_ENV=development nodemon -w src --exec \"babel-node src --presets env\"",
    "build": "babel src -s -D -d dist --presets env",
    "start": "node dist/index.js",
    "lint": "eslint src",
    "prettier": "prettier --write --single-quote --print-width=120 --tab-width=2 \"src/**/*.{js,jsx}\""
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "convert-seconds": "^1.0.1",
    "discord.js": "^11.2.1",
    "dotenv": "^4.0.0",
    "mongoose": "^4.13.9",
    "steem": "^0.7.1",
    "steem-upvote-util": "1.3.2",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.14.0",
    "nodemon": "^1.14.3"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-var": 1
    }
  },
  "engines": {
    "node": "8.9.2"
  },
  "name": "stephard",
  "version": "3.0.0",
  "main": "index.js",
  "license": "MIT"
}
